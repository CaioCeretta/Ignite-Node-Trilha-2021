{
  "compilerOptions": {
    "allowJs": true,                                  
    "jsx": "react-jsx",                                
    "noEmit": true,                                   
    "strict": true,      
    "strictPropertyInitialization": false,                                
    "moduleResolution": "node",                       
    "resolveJsonModule": true,                        
    "isolatedModules": true,                          
    "allowSyntheticDefaultImports": true,             
    "esModuleInterop": true,                             
    "skipLibCheck": true,                           
    "forceConsistentCasingInFileNames": true,            
  },
  "include": ["src"]
}
